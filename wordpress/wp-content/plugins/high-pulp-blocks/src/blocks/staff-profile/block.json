{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gs/staff-profile",
	"version": "0.1.0",
	"title": "Staff Profile",
	"category": "design",
	"icon": "id",
	"description": "Staff profile for staic block homework",
	"example": {},
	"textdomain": "staff-profile",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
  "attributes": {
	"name": {
	  "type": "string",
	  "source": "html",
	  "selector": "h3"
	},
	"jobTitle": {
	  "type": "string",
	  "source": "html",
	  "selector": "p"
	},
	"imageUrl": {
	  "type": "string",
	  "source": "attribute",
	  "selector": "img",
	  "attribute": "src"
	},
	"textColor": {
	  "type": "string",
	  "default": "#000000"
	},
	"backgroundColor": {
	  "type": "string",
	  "default": "#FFFFFF"
	},
	"imageSize": {
	  "type": "number",
	  "default": 150
	}
  }}


